{
  "name": "fuzzylite",
  "version": "0.3.0",
  "description": "A Node.js interface for the fuzzylite library",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "cd fuzzylite && git clean -x -f -d",
    "preinstall": "npm run build-fuzzylite",
    "pretest": "npm run lint",
    "test": "mocha --reporter spec --recursive",
    "lint": "jshint lib test",
    "test-watch": "mocha --recursive --invert --grep 'Integration' --watch --reporter min",
    "build-fuzzylite": "cd fuzzylite/fuzzylite && ./build.sh release"
  },
  "devDependencies": {
    "jshint": "^2.9.1-rc1",
    "mocha": "^1.21.4",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2"
  },
  "keywords": [
    "fuzzy",
    "fuzzylite",
    "node"
  ],
  "author": "Scypho",
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^2.2.0"
  }
}
